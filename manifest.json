{
  "name": "Background Music",
  "version": "1.0",
  "description": "Listen to some audio in the background.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Background Music",
    "popup": "popup.html"
  },
  "background_page": "background.html",
  "content_scripts": [{
	"matches": ["http://*/*", "https://*/*"],
	     "js": ["jquery.min.js", "content.js"],
     "all_frames": true
  }]
}
